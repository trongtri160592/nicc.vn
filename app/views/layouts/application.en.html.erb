<!-- HEADER -->
<header class="push" role="banner">
  <div id="nvcgSlSiteBanner" class="row">
    <div class="contentid-911584 slot-item only-SI nci-logo-pages large-12 columns">

      <%= link_to (image_tag 'NCILogongang-2.png', alt: 'National Cancer Institute', width: '60%'), root_path %>
    </div>
  </div>
</header>
<!-- END HEADER -->
<!-- Begin Page -->
<div id="page">
  <!-- Global nav/utilit bar/language  -->
  <div class="fixedtotop">
    <div class="headroom-area slide headroom--not-bottom headroom--top">
      <!-- LANGUAGE BAR -->
      <div class="language-bar">
        <div class="row sitewide-language">
          <div id="LangList1" class="large-12 columns">
            <ul>
              <li>
                <%= link_to_unless I18n.locale == :vi, "Tiếng Việt", locale: :vi %>
                |
                <%= link_to_unless I18n.locale == :en, "English", locale: :en %>
              </li>
            </ul>
          </div>
        </div>
      </div><!-- end "language-bar" -->
      <!-- END LANGUAGE BAR -->

    </div><!-- end "headroom-area" -->
    <!-- BEGIN NAVIGATION -->
    <!-- Begin nav-search bar -->
    <div class="nav-search-bar gradient header">
      <div id="nvcgSlMainNav" class="row">
        <div class="contentid-113330 slot-item first-SI">
          <div class="navigation">
            <div class="mobile-menu-bar">
              <button type="button" class="open-panel nav-header menu-btn">Menu</button>
              <button type="button" class="nav-search">Search</button>
            </div>
            <nav id="mega-nav" role="navigation">
              <ul class="menu nav-menu">
                <% if manage_category == 'about' %>
                  <li class="nav-item current-page lvl-1 has-children item-2">
                <% else %>
                  <li class="nav-item lvl-1 has-children item-2">
                <% end %>
                <div class="nav-item-title">
                  <%= link_to 'About us', about_summary_path %>
                </div>
                <ul class="mobile-item">
                  <li class="lvl-2">
                    <div class="nav-item-title">
                      <%= link_to "Summary", about_summary_path %>
                    </div>
                  </li>
                </ul>
                </li>
                <% if manage_category == 'type' %>
                  <li class="nav-item current-page lvl-1 has-children item-2">
                <% else %>
                  <li class="nav-item lvl-1 has-children item-2">
                <% end %>
                <div class="nav-item-title">
                  <%= link_to 'Cancer types', cancer_type_index_path %>

                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                <ul class="mobile-item">
                  <li class="lvl-2 has-children">
                    <div class="nav-item-title">
                      <a href="#">Common cancer types</a>
                      <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                      </button>
                    </div>
                    <ul class="mobile-item">
                      <% CancerType.where(is_common: 1).first(3).each do |cancer_type| %>
                        <li class="lvl-3">
                          <div class="nav-item-title">
                            <%= link_to cancer_type.name_english, cancer_type_detail_path(id: cancer_type.id) %>
                          </div>
                        </li>
                      <% end %>
                    </ul>
                  </li>
                  <li class="lvl-2">
                    <div class="nav-item-title">
                      <%= link_to "All cancer types (A-Z)", cancer_type_index_path %>
                    </div>
                  </li>
                </ul>
                <div aria-expanded="false" aria-haspopup="true" class="sub-nav-mega" id="accessible-megamenu-1526307612004-4" role="group" aria-hidden="true" aria-labelledby="accessible-megamenu-1526307612004-3">
                  <div class="sub-nav-group-wrapper">
                    <div class="sub-nav-group-subwrapper">
                      <ul class="sub-nav-group cancer-types-border-container">
                        <li><a href="#" class="sub-nav-group-header">Common cancer types</a>
                          <% CancerType.where(is_common: 1).first(12).in_groups_of(4, false).each do |common_arr| %>
                            <ul>
                              <% common_arr.each do |cancer_type| %>
                                <li><%= link_to cancer_type.name_english, cancer_type_detail_path(id: cancer_type.id) %></li>
                              <% end %>
                            </ul>
                          <% end %>
                        </li>
                      </ul>
                      <ul class="sub-nav-group">
                        <li><%= link_to 'All cancer types', cancer_type_index_path, class: 'sub-nav-group-header' %>
                          <ul>
                            <li><%= link_to 'List of types A-Z', cancer_type_index_path %></li>
                          </ul>
                        </li>
                      </ul>
                    </div>
                  </div>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                </li>
                <% if manage_category == 'research' %>
                  <li class="nav-item current-page lvl-1 has-children item-3">
                <% else %>
                  <li class="nav-item lvl-1 has-children item-3">
                <% end %>
                <div class="nav-item-title">
                  <%= link_to 'Research and training', research_path %>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                <ul class="mobile-item">
                  <% ResearchCategory.all.each do |category| %>
                    <li class="lvl-2 has-children">
                      <div class="nav-item-title">
                        <%= link_to category.name_english, research_category_path(id: category.id) %>
                      </div>
                    </li>
                  <% end %>
                </ul>
                <div aria-expanded="false" aria-haspopup="true" class="sub-nav-mega mega-menu-scroll" id="accessible-megamenu-1526307612015-6" role="group" aria-hidden="true" aria-labelledby="accessible-megamenu-1526307612015-5">
                  <div class="sub-nav-group-wrapper">
                    <div class="sub-nav-group-subwrapper">
                      <% ResearchCategory.all.each do |category| %>
                        <ul class="sub-nav-group">
                          <li><%= link_to category.name_english, research_category_path(id: category.id), class: "sub-nav-group-header" %>
                          </li>
                        </ul>
                      <% end %>
                    </div>
                  </div>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                </li>
                <% if manage_category == 'news' %>
                  <li class="nav-item current-page lvl-1 has-children item-5">
                <% else %>
                  <li class="nav-item lvl-1 has-children item-5">
                <% end %>
                <div class="nav-item-title">
                  <%= link_to 'News and event', news_path %>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                <% news_by_year = News.all.order(created_at: :desc)
                    .group_by {|news| news.created_at.year} %>
                <ul class="mobile-item">
                  <li class="lvl-2 has-children">
                    <div class="nav-item-title">
                      <a href="#">Sort by year</a>
                      <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                      </button>
                    </div>
                    <ul class="mobile-item">
                      <% news_by_year.keys.each do |year| %>
                        <li class="lvl-3">
                          <div class="nav-item-title">
                            <%= link_to year, news_filter_path(year: year) %>
                          </div>
                        </li>
                      <% end %>
                    </ul>
                  </li>
                </ul>
                <div aria-expanded="false" aria-haspopup="true" class="sub-nav-mega" id="accessible-megamenu-1526307612025-10" role="group" aria-hidden="true" aria-labelledby="accessible-megamenu-1526307612025-9">
                  <div class="sub-nav-group-wrapper">
                    <div class="sub-nav-group-subwrapper">
                      <ul class="sub-nav-group">
                        <li>
                          <a href="#" class="sub-nav-group-header">Sort by year</a>
                          <ul>
                            <% news_by_year.keys.each do |year| %>
                              <li><%= link_to year, news_filter_path(year: year) %></li>
                            <% end %>
                          </ul>
                        </li>
                      </ul>
                    </div>
                  </div>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                </li>
                <% if manage_category == 'person' %>
                  <li class="nav-item current-page lvl-1 has-children item-6">
                <% else %>
                  <li class="nav-item lvl-1 has-children item-6">
                <% end %>
                <div class="nav-item-title">
                  <%= link_to 'Human resources', person_path %>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                <ul class="mobile-item">
                  <% Leadership.all.each do |leadership| %>
                    <li class="lvl-2 has-children">
                      <div class="nav-item-title">
                        <%= link_to leadership.position_english, person_position_path(id: leadership.id) %>
                      </div>
                    </li>
                  <% end %>
                </ul>
                <div aria-expanded="false" aria-haspopup="true" class="sub-nav-mega mega-menu-scroll" id="accessible-megamenu-1526307612033-12" role="group" aria-hidden="true" aria-labelledby="accessible-megamenu-1526307612033-11">
                  <div class="sub-nav-group-wrapper">
                    <div class="sub-nav-group-subwrapper">
                      <% Leadership.all.each do |leadership| %>
                        <ul class="sub-nav-group">
                          <li><%= link_to leadership.position_english, person_position_path(id: leadership.id), class: "sub-nav-group-header" %>
                          </li>
                        </ul>
                      <% end %>
                    </div>
                  </div>
                  <button class="toggle" aria-expanded="false" type="button"><span class="hidden">Expand</span>
                  </button>
                </div>
                </li>
              </ul>
            </nav>
          </div>
        </div>
        <div class="contentid-911752 slot-item last-SI">
          <div class="rawHtml">
            <div class="search" role="search">
              <%= form_tag search_path, method: :get, id: 'siteSearchForm' do %>
                <%= label_tag :swKeyword, 'Search', class: 'hidden' %>
                <%= text_field_tag :swKeyword, params[:term], class: 'searchString ui-autocomplete-input', id: 'swKeyword', max_length: 255, placeholder: 'Search' %>
                <%= submit_tag '<span class="hidden">Search</span>'.html_safe, id: 'sitesearch', class: 'searchSubmit' %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- End nav-search bar -->

    <!-- END NAVIGATION -->
  </div>
  <div class="fixedtotop-spacer" style="display: none; width: 1907px; height: 116.6px; float: none;"></div><!-- END Global nav/utility bar/language -->
  <%= yield %>
</div> <!-- END Page -->

