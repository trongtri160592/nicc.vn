  <%= javascript_include_tag 'application', defer: 'defer' %>
</head>
<body id="Body1" class="ncihome" data-cde-contenttype="ncihome" data-cde-pagetemplate="NVCGHomePageTemplate" data-cde-templatetheme="default" data-cde-contentid="13019">
