<a class="btn btn-primary" href="/partner/new"><i class="fa fa-plus" aria-hidden="true"></i></a></br>
<table class="table table-bordered table-striped">
  <thead>.
  <tr>
    <th>Tên</th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @partners.each do |partner| %>
    <tr>
      <td><%= partner.name %></td>
      <td><%= link_to ("<i class='fa fa-pencil-square'></i>&nbsp;Sửa").html_safe, "/partner/edit/#{partner.id}", class: "btn btn-primary" %></td>
      <td><%= link_to ("<i class='fa fa-trash'></i></i>&nbsp;Xóa").html_safe, "/partner/#{partner.id}", method: :delete, class: "btn btn-danger", confirm: "Có phải bạn muốn xóa '#{partner.name}'?" %></td>
    </tr>
  <% end %>
  </tbody>

</table>